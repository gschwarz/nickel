			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="Configuraci&oacute;n del proceso"/>
			</g:Cell>
		</g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Cell verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="Fecha de inicio :" width="114px" horizontalAlignment="ALIGN_LEFT"/>
			</g:Cell>
			<dp:DateBox ui:field="initDate" />
		</g:HorizontalPanel>
